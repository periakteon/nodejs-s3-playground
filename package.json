{
    "name": "nodejs-s3-playground",
    "description": "Playground for AWS S3 with Node.js",
    "version": "1.0.0",
    "main": "index.js",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
        "lint": "eslint .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "start": "node ./dist/server.js",
        "build": "cross-env NODE_ENV=production tsc --build"
    },
    "keywords": [],
    "author": " ",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.20.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "class-validator-jsonschema": "^5.0.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^8.0.0",
        "mongodb": "^6.9.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.3",
        "reflect-metadata": "^0.2.2",
        "routing-controllers": "^0.10.4",
        "routing-controllers-openapi": "^4.0.0",
        "swagger-ui-express": "^5.0.1",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "winston": "^3.15.0",
        "winston-daily-rotate-file": "^5.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.12.0",
        "@types/body-parser": "^1.19.5",
        "@types/express": "^4.17.21",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^18.14.6",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.30",
        "@typescript-eslint/parser": "^8.0.0-alpha.30",
        "cross-env": "^7.0.3",
        "eslint": "^9.5.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "globals": "^15.11.0",
        "prettier": "^3.3.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5"
    }
}
